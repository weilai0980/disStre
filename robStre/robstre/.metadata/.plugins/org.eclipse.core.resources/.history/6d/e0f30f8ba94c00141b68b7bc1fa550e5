import java.io.IOException;



public class coorGenerationTest {
	
	int[] direcVec = {-1,0,1};
	int[] a = {1,2,3};
	
	void calCellCoor( int dftNum,int curCnt, String cellCoor) {

		if(curCnt>=dftNum)
		{
			
			
			return;
			
		}
		for(int i=0;i<3;++i)
		{
			cellCoor=cellCoor+ Integer.toString(a[curCnt]+direcVec[i])+",";
			
			calCellCoor(dftNum, curCnt+1, cellCoor);
		
		}
	
		return;
	}

	
	public static void main(String[] args)  {
	
	}
	

}
